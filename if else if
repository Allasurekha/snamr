#include<stdio.h>
main()
{
int marks;
printf("enter marks");
scanf("%d",&marks);
if(marks>=85 && marks<=100) 
printf("gradeA");
else if(marks<85 && marks>=70)
printf("gradeB");
else if(marks<70 && marks>=55)
printf("gradeC");
else if(marks<55 && marks>=40)
printf("gradeD");
else
printf("gradeF");
return0;
}
